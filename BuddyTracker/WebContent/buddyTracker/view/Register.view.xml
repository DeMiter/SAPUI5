<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="sap.ui.trident.buddyTracker.view.Register" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{i18n>RegisterTitle}">
		<content>
			<Label text="{i18n>ScreenName}" />
			<Input id="screenName" type="text" placeHolder="{i18n>ScreenNameInput}" />
			<Label text="{i18n>Email}" />
			<Input id="email" type="text" placeHolder="{i18n>EmailInput}" />
			<Label text="{i18n>User}" />
			<Input id="user" type="text" placeHolder="{i18n>UserInput}" />
			<Label text="{i18n>Password}" />
			<Input id="password" type="password" placeHolder="{i18n>PasswordInput}" />
			<Button text="{i18n>SignUp}" press="handleSignUp" />
			<Button text="{i18n>Cancel}" press="handleCancel" />
		</content>
	</Page>
</core:View>